<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hxzz.demo.mapper.Quality3Mapper">
    <select id="showMissing" resultType="com.hxzz.demo.entity.Quality3">
    select * from Quality3 where  (id=(select MAX(id) from (select id from Quality3 where name="总装责任错漏装")) )

    </select>
    <select id="showIntercept" resultType="com.hxzz.demo.entity.Quality3">
        select * from Quality3 where  (id=(select MAX(id) from (select id from Quality3 where name="总装拦截问题")) )
    </select>
    <select id="getMissingData"  resultType="com.hxzz.demo.entity.Quality3">
        select *  from Quality3  where (date &gt;=#{date1} and date  &lt;= #{date2}) and (name="总装责任错漏装")
    </select>
    <select id="getInterceptData" resultType="com.hxzz.demo.entity.Quality3">
        select *  from Quality3  where (date &gt;=#{date1} and date  &lt;= #{date2}) and (name="总装拦截问题")
    </select>
    <insert id="add">
        insert into Quality3(name,DC,DD,XB,XC) values (#{name},#{DC},#{DD},#{XB},#{XC})
    </insert>
    <delete id="del">
        delete  from Quality3 where id=#{id}
    </delete>
    <update id="change">
        update Quality3 set(name=#{name},DC=#{DC},DD=#{DD},XB=#{XB},XC=#{XC}) where id=#{id}
    </update>

</mapper>
