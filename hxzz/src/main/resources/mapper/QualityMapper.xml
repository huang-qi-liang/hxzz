<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hxzz.demo.mapper.QualityMapper">
    <select id="show" resultType="com.hxzz.demo.entity.Quality">
        select * from Quality where date=#{date}
    </select>
    <select id="getData" resultType="com.hxzz.demo.entity.Quality">
        select *  from  Quality where ((date &gt;= #{date1} )and (date &lt;= #{date2}))
    </select>
    <insert id="add">
        insert into Quality( name, offLine, dLine, lingYu, careLine, roadTest) values
        (#{name},#{offLine},#{dLine},#{lingYu},#{careLine},#{roadTest})
    </insert>
    <delete id="del">
        delete from Quality where id=#{id}
    </delete>
    <update id="change">
        update Quality
        set(name=#{name},offLine=#{offLine},dLine=#{dLine},lingYu=#{lingYu},careLine=#{careLine},roadTest=#{roadTest})
        where id=#{id}
    </update>

</mapper>
